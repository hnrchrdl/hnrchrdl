doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(name='csrf-token', content=_csrf)
    title #{title} - hnrchrdl
    if title == 'HH Trees'
      != css('vendor/cartodb/cartodb')
      != css('vendor/dcjs/dc')
    != css('main')
  body
    if title != 'HH Trees'
      include partials/header

    .container
      //include partials/flash
      block content

    //include partials/footer

    != js('application')
    if title == 'HH Trees'
      != js('lib/d3')
      != js('lib/crossfilter')
      != js('lib/dc')
      != js('hhtrees')
